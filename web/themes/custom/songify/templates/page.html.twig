<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">

<header id="site-header">
  <div id="site-logo">
    <a href="/" title="{{ site_name }}">
      <img src="/themes/custom/songify/assets/images/songify-logo.png" alt="{{ site_name }}" />
    </a>
  </div>
  <div id="site-navigation">
    {% for key, value in page.header %}
      {% if 'songify' in key %}
        {{ value }}
      {% endif %}
    {% endfor %}
  </div>
</header>
<main {{ attributes.addClass() }}>
  {% block content %}
    <div class="custom-content">
      <p>{{ custom_variable }}</p>
    </div>
    {{ page.content }}
  {% endblock %}
</main>
<footer>
  {{ page.footer }}
</footer>
